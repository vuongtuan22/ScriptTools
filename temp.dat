#!/bin/bash

remote_command()
{   
   expect -c "
        set timeout -1 
        spawn ssh $2@$1
        expect {
            -re \".*Are.*.*yes.*no.*\" { send \"yes\r\"; exp_continue }
            \"password: \" { send \"$3\r\" }
            eof { exit }
        }
        expect {
            -re \"password:*\" { send \"$4\r\"; exp_continue }
            eof { exit }
        }
        expect {
            -re \"password:*\" { send \"$5\r\"; exp_continue }
            eof { exit }
        }

        expect {
            -re \"raspberrypi*\" { send \"exit\r\"; exp_continue }
            eof { exit }
        }


        expect -re \"(%|#|\\\\$) \" { exit }
    "
}

remote_command2()
{
