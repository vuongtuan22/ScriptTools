            -re \".*Are.*.*yes.*no.*\" { send \"yes\r\"; exp_continue }
            \"password: \" { send \"$5\r\" }
            -re \"pi.raspberrypi\" { exit }
            eof { exit }
        }

        expect -re \"pi.raspberrypi\" { exit }
    "
}

